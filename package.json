{
  "name": "with-rn-bluetooth-classic",
  "version": "1.0.5",
  "description": "Config plugin for auto configuring react-native-bluetooth-classic on Expo",
  "main": "plugin/lib",
  "scripts": {
    "clean": "cd plugin && rd /s /q lib",
    "clean-mac-lin": "cd plugin && rm -rf lib",
    "build": "cd plugin && npx tsc",
    "rebuild": "npm run clean && npm run build",
    "rebuild-max-lin": "npm run clean-mac-lin && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amitferman/with-bluetooth-classic.git"
  },
  "keywords": [
    "Expo",
    "Config Plugin",
    "Bluetooth Classic"
  ],
  "author": "Amit Ferman <aferman@uw.edu> (https://github.com/amitferman)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amitferman/with-bluetooth-classic/issues"
  },
  "homepage": "https://github.com/amitferman/with-bluetooth-classic#readme",
  "dependencies": {
    "@expo/config-plugins": "^9.0.0"
  },
  "devDependencies": {
    "expo-module-scripts": "^4.1.7",
    "typescript": "^4.3.5"
  },
  "files": [
    "plugin",
    "app.plugin.js"
  ]
}
